{
    "manifest_version": 3,
    "name": "BiasEd",
    "version": "1.0",
    "description": "Recognizes bias online",
    "permissions": [
        "activeTab"
    ],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "service-worker.js"
    },  
    "content_scripts": [
        {
          "js": ["content.js"],
          "matches": [
            "<all_urls>"
          ]
        }
    ]
}  